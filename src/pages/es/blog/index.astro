---
import Base from '../../../layouts/Base.astro';
import Blogs from '../../../components/Blog/Blogs.astro';
const posts = Astro
      .fetchContent('./*.md')
      .sort((a, b) => {
        return (
          new Date(b.created).getTime() -
            new Date(a.created).getTime()
        )
      });
---
<Base title="Blog" lang="es" section="blog">
  <Blogs lang="es" {posts} />
</Base>
